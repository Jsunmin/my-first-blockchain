    "@polkadot/api": "^7.9.1",
    "dotenv": "^16.0.0",
    "ethers": "^5.5.4",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.2.42",
    "web3": "^1.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.17",
    "ganache": "^7.0.2",
